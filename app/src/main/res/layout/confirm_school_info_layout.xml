<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_horizontal"
    android:background="#00caca"
    android:orientation="vertical">
    <TextView
        android:id="@+id/confirm_school_info_title"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="9%w"
        app:layout_textSizePercent="6%w"
        android:gravity="center"
        android:textColor="#ecf0f1"
        android:text="@string/confirm_school_info_title"/>
    <include layout="@layout/confirm_school_info_layout_item"/>
</com.zhy.android.percent.support.PercentLinearLayout>
